<phpunit>
	<testsuites>
 		 <testsuite name="unittests">
			<directory>./tests/unit</directory>
        </testsuite>
	    <testsuite name="end2endtests">
            <directory>./tests/e2e</directory>
        </testsuite>
    </testsuites>
    <filter>
        <whitelist>
            <directory>./src</directory>
        </whitelist>
    </filter>
    <groups>
        <exclude>
            <group>e2eSubtest</group>
        </exclude>
    </groups>
</phpunit>
